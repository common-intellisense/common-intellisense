{
  "publisher": "simonhe",
  "name": "common-intellisense",
  "displayName": "%displayName%",
  "version": "0.3.8",
  "packageManager": "pnpm@10.24.0",
  "description": "%description%",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/common-intellisense/common-intellisense#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/common-intellisense/common-intellisense.git"
  },
  "bugs": {
    "url": "https://github.com/common-intellisense/common-intellisense/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "keywords": [
    "VSCode",
    "Intellisense",
    "UI框架",
    "智能提示",
    "插件",
    "Element UI",
    "Ant Design",
    "Vuetify",
    "Chakra UI",
    "PrimeVue",
    "Quasar",
    "NextUI",
    "NuxtUI",
    "Shadcn",
    "Radix",
    "Arco Design",
    "UView",
    "Taro",
    "NutUI",
    "Vant",
    "MUI",
    "Naive UI",
    "Varlet",
    "Bits UI",
    "Opentiny",
    "TDesign",
    "HeadlessUI",
    "MantineUI",
    "WotDesign",
    "Ark UI",
    "Dcloudio UniUi",
    "Dcloudio UniApp"
  ],
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.75.1"
  },
  "activationEvents": [
    "workspaceContains:package.json"
  ],
  "contributes": {
    "configuration": {
      "title": "%configurationTitle%",
      "properties": {
        "common-intellisense.ui": {
          "type": "array",
          "default": [
            "auto"
          ],
          "items": {
            "type": "string",
            "enum": [
              "auto",
              "elementUi2",
              "elementPlus2",
              "antd4",
              "antd5",
              "vant4",
              "naiveUi2",
              "antDesignVue4",
              "antDesignVue3",
              "antDesignVue2",
              "antDesignVue1",
              "varlet2",
              "varlet3",
              "vuetify3",
              "chakraUiReact2",
              "primevue3",
              "primevue4",
              "quasar2",
              "nextui2",
              "nuxtui2",
              "shadcnVue0",
              "shadcnSvelte0",
              "bitsUi0",
              "radixVue1",
              "arcoDesign2",
              "arcoDesignVue2",
              "uview1",
              "uview2",
              "@opentiny/vue3",
              "@tarojs/components3",
              "@nutui/nutui4",
              "@vant/weapp1",
              "tdesignReact1",
              "tdesignMobileVue1",
              "tdesinVueNext1",
              "tdesignMiniprogram1",
              "@headlessui/react2",
              "@headlessui/vue1",
              "@mui/material5",
              "@chakraUi/react2",
              "@mantine/core7",
              "@mantine/dates7",
              "@mantine/charts7",
              "wotDesignUi1",
              "arkUi4",
              "dcloudioUniUi"
            ]
          },
          "description": "%uiDescription%"
        },
        "common-intellisense.translate": {
          "type": "boolean",
          "default": false,
          "description": "%useTranslateDescription%"
        },
        "common-intellisense.showSlots": {
          "type": "boolean",
          "default": true,
          "description": "%showSlotsDescription%"
        },
        "common-intellisense.prefix": {
          "type": "object",
          "default": {},
          "description": "%prefixDescription%"
        },
        "common-intellisense.alias": {
          "type": "object",
          "default": {},
          "description": "%aliasDescription%"
        },
        "common-intellisense.exclude": {
          "type": "array",
          "default": [
            "**/dist/**",
            "**/node_modules/**"
          ],
          "description": "%excludeDescription%"
        },
        "common-intellisense.remoteUris": {
          "type": "array",
          "default": [],
          "description": "%remoteUris%"
        },
        "common-intellisense.remoteNpmUris": {
          "type": "array",
          "default": [],
          "description": "%remoteNpmUris%"
        },
        "common-intellisense.localUris": {
          "type": "array",
          "default": [],
          "description": "%localUris%"
        }
      }
    },
    "commands": [
      {
        "command": "common-intellisense.pickUI",
        "title": "%pickUITitle%"
      },
      {
        "command": "common-intellisense.cleanCache",
        "title": "%cleanLocalCache%"
      }
    ]
  },
  "scripts": {
    "dev": "tsup src/index.ts --external vscode --watch cjs",
    "test": "vitest",
    "translate": "node scripts/translate-directives.js",
    "translate:report": "node scripts/translate-directives.js --report",
    "translate:report:diff": "node scripts/translate-directives.js --report --diff",
    "translate:apply": "node scripts/translate-directives.js --apply --report",
    "test:translate": "vitest run test/scripts/translate-directives.test.ts",
    "build": "tsup src/index.ts --external vscode --minify --format cjs",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "prepare": "husky install",
    "test:staged": "git diff --staged --name-only --relative | grep -E '\\.(ts|tsx|js)$' | xargs -r pnpm -s exec vitest related",
    "lint:fix": "eslint . --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@rollup/pluginutils": "^5.3.0",
    "@simon_he/fetch-npm": "^0.0.25",
    "@simon_he/fetch-npm-cjs": "^0.0.30",
    "@simon_he/latest-version": "^0.1.15",
    "@simon_he/translate": "^0.0.13",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.19.130",
    "@types/vscode": "1.75.1",
    "@typescript-eslint/typescript-estree": "^8.48.1",
    "@vscode-use/createwebview": "^0.0.13",
    "@vscode-use/utils": "0.1.47",
    "@vscode/vsce": "^3.7.1",
    "@vue-vine/compiler": "^0.1.42",
    "@vue/compiler-sfc": "3.5.18",
    "bing-translate-api": "^2.10.0",
    "bumpp": "^9.11.1",
    "eslint": "^9.39.1",
    "fast-glob": "^3.3.3",
    "find-up": "^6.3.0",
    "get-lib-version": "^0.0.2",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.1",
    "lazy-js-utils": "^0.1.49",
    "lint-staged": "^14.0.1",
    "ofetch": "^1.5.1",
    "prettier": "^3.7.4",
    "svelte": "^5.45.6",
    "tsup": "^6.7.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
