{
  "publisher": "simonhe",
  "name": "common-intellisense",
  "displayName": "%displayName%",
  "version": "0.2.5",
  "packageManager": "pnpm@8.15.9",
  "description": "%description%",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/common-intellisense/common-intellisense#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/common-intellisense/common-intellisense"
  },
  "bugs": {
    "url": "https://github.com/common-intellisense/common-intellisense/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "keywords": [
    "VSCode",
    "Intellisense",
    "UI框架",
    "智能提示",
    "插件",
    "Element UI",
    "Ant Design",
    "Vuetify",
    "Chakra UI",
    "PrimeVue",
    "Quasar",
    "NextUI",
    "NuxtUI",
    "Shadcn",
    "Radix",
    "Arco Design",
    "UView",
    "Taro",
    "NutUI"
  ],
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.77.0"
  },
  "activationEvents": [
    "workspaceContains:package.json"
  ],
  "contributes": {
    "configuration": {
      "title": "%configurationTitle%",
      "properties": {
        "common-intellisense.ui": {
          "type": "array",
          "default": [
            "auto"
          ],
          "items": {
            "type": "string",
            "enum": [
              "auto",
              "elementUi2",
              "elementPlus2",
              "antd4",
              "antd5",
              "vant4",
              "naiveUi2",
              "antDesignVue4",
              "antDesignVue3",
              "antDesignVue2",
              "antDesignVue1",
              "varlet2",
              "varlet3",
              "vuetify3",
              "chakraUiReact2",
              "primevue3",
              "quasar2",
              "nextui2",
              "nuxtui2",
              "shadcnVue0",
              "radixVue1",
              "arcoDesign2",
              "arcoDesignVue2",
              "uview1",
              "uview2",
              "@opentiny/vue3",
              "@tarojs/components3",
              "@nutui/nutui4"
            ]
          },
          "description": "%uiDescription%"
        },
        "common-intellisense.showSlots": {
          "type": "boolean",
          "default": true,
          "description": "%showSlotsDescription%"
        },
        "common-intellisense.alias": {
          "type": "object",
          "default": {},
          "description": "%aliasDescription%"
        },
        "common-intellisense.exclude": {
          "type": "array",
          "default": [
            "**/dist/**",
            "**/node_modules/**"
          ],
          "description": "%excludeDescription%"
        },
        "common-intellisense.remoteUris": {
          "type": "array",
          "default": [],
          "description": "%remoteUris%"
        },
        "common-intellisense.localUris": {
          "type": "array",
          "default": [],
          "description": "%localUris%"
        }
      }
    },
    "commands": [
      {
        "command": "common-intellisense.pickUI",
        "title": "%pickUITitle%"
      },
      {
        "command": "common-intellisense.cleanCache",
        "title": "%cleanLocalCache%"
      }
    ]
  },
  "scripts": {
    "dev": "tsup src/index.ts --external vscode --watch",
    "test": "vitest",
    "build": "tsup src/index.ts --external vscode --minify",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@babel/types": "^7.25.6",
    "@rollup/pluginutils": "^5.1.0",
    "@simon_he/latest-version": "^0.1.0",
    "@types/node": "^18.19.50",
    "@types/vscode": "1.77.0",
    "@typescript-eslint/typescript-estree": "^6.21.0",
    "@vscode-use/createwebview": "^0.0.13",
    "@vscode-use/utils": "0.1.29",
    "@vscode/vsce": "^3.1.0",
    "@vue-vine/compiler": "^0.1.21",
    "@vue/compiler-sfc": "3.3.4",
    "bing-translate-api": "^2.10.0",
    "bumpp": "^9.5.2",
    "eslint": "^8.57.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.3.2",
    "find-up": "^6.3.0",
    "ofetch": "^1.3.4",
    "pnpm": "^8.15.9",
    "prettier": "^3.3.3",
    "rimraf": "^4.4.1",
    "svelte": "^4.2.19",
    "translate-google": "^1.5.0",
    "tsup": "^6.7.0",
    "typescript": "^5.6.2",
    "vitest": "^0.29.8"
  }
}
